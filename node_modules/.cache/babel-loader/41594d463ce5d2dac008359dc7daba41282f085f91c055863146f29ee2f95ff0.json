   } catch (error) {\n        console.error('Error al obtener los prospectos:', error);\n      }\n    },\n    verDetalle(prospecto) {\n      // Navegar a la pantalla de detalles del prospecto utilizando Vue Router\n        this.$router.push(`/ProspectosEspecificos/${prospecto}`);\n\n    }\n  }\n};\n</script>\n\n\n<style>\n  /* Estilos para la tabla */\n  table {\n    width: 100%;\n    border-collapse: collapse;\n    border-spacing: 0;\n    border: 1px solid #ddd;\n    margin: auto; /* Centra la tabla horizontalmente */\n  }\n  .Eye{\n    display: inline-block;\n    width: 2em; \n    height: 2em; \n   \n    background-size: cover;\n    border: none;\n  }\n  .Agregar{\n    display: inline-block;\n    width: 5em; \n    height: 5em; \n    background-image: url('../assets/Persona.png');\n    background-size: cover;\n    border: none;\n  }\n  /* Estilos para las celdas del encabezado */\n  th {\n    padding: 12px;\n    text-align: center;\n    background-color: #f2f2f2;\n    border: 1px solid #ddd;\n  }\n\n  /* Estilos para las celdas de datos */\n  td {\n    padding: 8px;\n    border: 1px solid #ddd;\n  }\n\n  /* Estilos para las filas alternas */\n  tr:nth-child(even) {\n    background-color: #f2f2f2;\n  }\n</style>\n"],"mappings":";AAkCA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,MAAM,EAAE;MACpB,QAAQA,MAAM;QACZ,KAAK,CAAC;UACJ,OAAO,aAAa;QACtB,KAAK,CAAC;UACJ,OAAO,YAAY;QACrB,KAAK,CAAC;UACJ,OAAO,WAAW;QACpB;UACE,OAAO,oBAAoB;MAC/B;IACF,CAAC;IACD,MAAMH,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,6BAA6B,CAAC;QAC/D,IAAI,CAACP,UAAS,GAAIM,QAAQ,CAACP,IAAI;MACjC,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC1D;IACF,CAAC;IACDE,UAAUA,CAACC,SAAS,EAAE;MACpB;MACE,IAAI,CAACC,OAAO,CAACC,IAAI,CAAE,0BAAyBF,SAAU,EAAC,CAAC;IAE5D;EACF;AACF,CAAC","ignoreList":[]},"metad